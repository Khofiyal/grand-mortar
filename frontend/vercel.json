{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/app.jsx"
        },
        {
            "src": "/[^.]+", "dest": "/", "status": 200
        }
    ]
}